  "operators": [
    { "name": "*", "description": "Multiply/speed up - plays the pattern faster. `*2` plays twice as fast (twice per cycle)", "examples": ["s(\"bd*4\")", "note(\"c3 e3 g3\")*2"], "category": "operator" },
    { "name": "/", "description": "Divide/slow down - stretches the pattern over multiple cycles. `/2` plays over 2 cycles", "examples": ["s(\"bd sd\")/2", "note(\"c3 e3 g3 b3\")/4"], "category": "operator" },
    { "name": "@", "description": "Elongate - sets temporal weight relative to other events. `@2` makes event twice as long", "examples": ["note(\"c3@2 e3 g3\")", "s(\"bd@3 sd\")"], "category": "operator" },
    { "name": "!", "description": "Replicate - repeats the event without speeding up (unlike *)", "examples": ["note(\"c3!3 e3\")", "s(\"bd!4\")"], "category": "operator" },
    { "name": "?", "description": "Random removal - 50% chance to remove event. Add probability: `?0.25` for 25% removal", "examples": ["s(\"hh*8?\")", "note(\"c3 e3? g3?0.25\")"], "category": "operator" },
    { "name": "|", "description": "Random choice - randomly selects between alternatives each cycle", "examples": ["note(\"c3|e3|g3\")", "s(\"bd|sd|hh\")"], "category": "operator" },
    { "name": "~", "description": "Rest/silence - produces no sound for that step", "examples": ["s(\"bd ~ sd ~\")", "note(\"c3 ~ e3 ~\")"], "category": "operator" },
    { "name": ",", "description": "Parallel/polyphony - plays multiple patterns simultaneously (chords)", "examples": ["note(\"c3,e3,g3\")", "s(\"bd sd, hh*4\")"], "category": "operator" },
    { "name": "[", "description": "Open bracket - groups events to subdivide time equally", "examples": ["s(\"bd [sd sd] bd sd\")", "note(\"[c3 e3] g3\")"], "category": "operator" },
    { "name": "]", "description": "Close bracket - ends a group started with [", "examples": ["s(\"[bd bd] sd\")", "note(\"c3 [e3 g3]\")"], "category": "operator" },
    { "name": "<", "description": "Open angle bracket - alternates through items each cycle", "examples": ["note(\"<c3 e3 g3>\")", "s(\"<bd sd> hh\")"], "category": "operator" },
    { "name": ">", "description": "Close angle bracket - ends alternating pattern started with <", "examples": ["s(\"bd <sd cp>\")", "note(\"<c3 d3 e3>\")"], "category": "operator" },
    { "name": "(", "description": "Open parenthesis - starts Euclidean rhythm: (pulses,steps,offset)", "examples": ["s(\"bd(3,8)\")", "s(\"hh(5,8,2)\")"], "category": "operator" },
    { "name": ")", "description": "Close parenthesis - ends Euclidean rhythm definition", "examples": ["s(\"bd(3,8) sd(2,8)\")", "s(\"cp(3,8,1)\")"], "category": "operator" },
    { "name": ":", "description": "Sample selector - selects specific sample variation by index", "examples": ["s(\"hh:0 hh:1 hh:2\")", "s(\"bd:3\")"], "category": "operator" }
  ]
}
